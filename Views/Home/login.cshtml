@{
    ViewData["Title"] = "Home Page";
}

<main class="pagina-login-register">
<div class="container">
        <h1>Logearse</h1>
        <form action='@Url.Action("LogearUsuario", "Home")' method="post">
            <div class="form-floating mb-3">
  <input type="email" class="form-control" id="floatingInput" autocomplete="off" placeholder="" name="mail">
  <label for="floatingInput">Mail</label>
</div>
            <div class="form-floating mb-3">
  <input type="password" class="form-control" id="floatingInput" autocomplete="off" placeholder="" name="contra">
  <label for="floatingInput">Contraseña</label>
</div>
@Html.Raw(ViewBag.error)
            <button type="submit">Logearse</button>
        </form>
    </div>

</main>
  


 
 